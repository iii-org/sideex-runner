{
  "input": {
    "testSuites": [
      "repo/iiidevops/sideex/1.sideex.json",
      "repo/iiidevops/sideex/2.sideex.json",
      "repo/iiidevops/sideex/sideex.json"
    ],
    "variables": [
      "./variables.json"
    ]
  },
  "webdriver": {
    "configs": [
      {
        "serverUrl": "http://localhost:4444",
        "type": "selenium",
        "browsers": [
          {
            "active": true,
            "capability": {
              "browserName": "chrome",
              "goog:chromeOptions": {
                "args": [
                  "headless",
                  "disable-dev-shm-usage",
                  "no-sandbox"
                ]
              }
            }
          }
        ]
      }
    ]
  },
  "report": {
    "path": "report/",
    "type": "html",
    "snapshot": 1
  }
}